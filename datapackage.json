{
    "name": "landnr",
    "title": "Landnúmer from Staðfangaskrá",
    "description": "Building records for Iceland. Each record holds a landnúmer (land plot id), building id, street address, post code, and the longitude and latitude for the building's centre point. Each building can be uniquely identified using a combination of the landnúmer and street address.",
    "last_modified": "2013-09-02",
    "datapackage_version": "0.1.2",
    "sources": [{
        "name": "Þjóðskrá",
        "web": "http://opingogn.is/dataset/stadfangaskra"
    }],
    "resources": [
        {
            "path": "data/landnr.json",
            "schema": {
                "fields": [
                    {
                        "id": "landnr",
                        "label": "Landnúmer",
                        "type": "integer"
                    },
                    {
                        "id": "building_id",
                        "label": "Building id",
                        "type": "integer"
                    },
                    {
                        "id": "street",
                        "label": "Street address",
                        "type": "string"
                    },
                    {
                        "id": "postcode",
                        "label": "Post code",
                        "type": "integer"
                    },
                    {
                        "id": "ll",
                        "label": "Longitude and latitude",
                        "type": "geopoint"
                    }
                ]
            }
        }
    ]
}
